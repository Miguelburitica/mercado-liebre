<% for (let i = 0; i < suggest.length; i++) { %>
    <article class="control-article">
        <img src="/images/products/<%= suggest[i].image%>" alt="<%= suggest[i].name %>" />
        <p class="product-name"><%= suggest[i].name %></p>
        <p Class="price">Desde $<%= suggest[i].price %></p>
    </article>
    <a href="/products/detail/<%= suggest[i].id %>"></a>
<% }%>